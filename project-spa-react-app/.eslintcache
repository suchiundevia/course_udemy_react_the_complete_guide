teId = params.quoteId;\n\n  useEffect(() => {\n    sendRequest(quoteId);\n  }, [sendRequest, quoteId]);\n\n  const quote = DUMMY_QUOTES.find((quote) => quote.id === params.quoteId);\n\n  if (status === \"pending\") {\n    return (\n      <div className=\"centered\">\n        <LoadingSpinner />\n      </div>\n    );\n  }\n  if (error) {\n    return <p className=\"centered\">{error}</p>;\n  }\n\n  if (!loadedQuote.text) {\n    return <p>No quote found!</p>;\n  }\n\n  return (\n    <Fragment>\n      <HighlightedQuote text={loadedQuote.text} author={loadedQuote.author} />\n      <Route path={match.path} exact>\n        <div className=\"centered\">\n          <Link className=\"btn--flat\" to={`${match.url}/comments`}>\n            View Comments\n          </Link>\n        </div>\n      </Route>\n\n      <p>{params.quoteId}</p>\n      <Route path={`${match.path}/comments`}>\n        <Comments />\n      </Route>\n    </Fragment>\n  );\n};\n\nexport default QuoteDetail;\n","/Users/suchiundevia/Documents/course_udemy_react_the_complete_guide/project-spa-react-app/src/pages/NewQuote.js",[],"/Users/suchiundevia/Documents/course_udemy_react_the_complete_guide/project-spa-react-app/src/components/comments/Comments.js",[],"/Users/suchiundevia/Documents/course_udemy_react_the_complete_guide/project-spa-react-app/src/components/comments/NewCommentForm.js",[],"/Users/suchiundevia/Documents/course_udemy_react_the_complete_guide/project-spa-react-app/src/components/layout/MainNavigation.js",[],"/Users/suchiundevia/Documents/course_udemy_react_the_complete_guide/project-spa-react-app/src/components/layout/Layout.js",[],"/Users/suchiundevia/Documents/course_udemy_react_the_complete_guide/project-spa-react-app/src/components/quotes/QuoteList.js",[],"/Users/suchiundevia/Documents/course_udemy_react_the_complete_guide/project-spa-react-app/src/components/quotes/QuoteItem.js",[],"/Users/suchiundevia/Documents/course_udemy_react_the_complete_guide/project-spa-react-app/src/components/quotes/